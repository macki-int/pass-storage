<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" prefHeight="376.0" prefWidth="543.0" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.mj.passstorage.controller.PassStorageController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <GridPane>
     <columnConstraints>
       <ColumnConstraints hgrow="SOMETIMES" maxWidth="669.0" minWidth="10.0" prefWidth="511.0" />
       <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" maxWidth="395.0" minWidth="10.0" prefWidth="296.0" />
     </columnConstraints>
     <rowConstraints>
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
     </rowConstraints>
      <children>
          <Button fx:id="buttonLogin" alignment="CENTER" contentDisplay="RIGHT" onAction="#onLoginButtonAction" prefHeight="25.0" prefWidth="118.0" text="Login" GridPane.columnIndex="1" GridPane.rowIndex="1">
            <GridPane.margin>
               <Insets left="10.0" />
            </GridPane.margin></Button>
         <TextField fx:id="textName" prefHeight="25.0" prefWidth="551.0" promptText="Name" />
         <PasswordField fx:id="textPassword" promptText="Password" GridPane.rowIndex="1" />
      </children>
   </GridPane>
   <TableView prefHeight="276.0" prefWidth="518.0">
     <columns>
       <TableColumn prefWidth="100.0" text="Nazwa" />
       <TableColumn prefWidth="100.0" text="Użytkownik" />
         <TableColumn prefWidth="100.0" text="Hasło" />
         <TableColumn prefWidth="200.0" text="Link" />
     </columns>
   </TableView>
</VBox>
